{
  "Carousel": {
    "align": {
      "type": "type ONLY_FOR_FORMAT =\n  | number\n  | \"center\"\n  | \"start\"\n  | \"end\"\n  | ResponsiveObject<number | \"center\" | \"start\" | \"end\">\n  | ColorModeArray<number | \"center\" | \"start\" | \"end\">",
      "defaultValue": "'center'",
      "required": false,
      "description": "The alignment of the carousel."
    },
    "autoplay": {
      "type": "type ONLY_FOR_FORMAT =\n  | boolean\n  | ResponsiveObject<boolean>\n  | ColorModeArray<boolean>",
      "defaultValue": false,
      "required": false,
      "description": "If `true`, the carousel will be autoplay."
    },
    "colorScheme": {
      "type": "\"primary\" | \"secondary\" | \"warning\" | \"danger\" | \"link\" | \"whiteAlpha\" | \"blackAlpha\" | \"gray\" | \"red\" | \"orange\" | \"yellow\" | \"green\" | \"teal\" | \"blue\" | \"cyan\" | \"purple\" | \"pink\"",
      "required": false,
      "description": "The visual color appearance of the component."
    },
    "containScroll": {
      "type": "type ONLY_FOR_FORMAT =\n  | \"\"\n  | \"trimSnaps\"\n  | \"keepSnaps\"\n  | ResponsiveObject<\"\" | \"trimSnaps\" | \"keepSnaps\">\n  | ColorModeArray<\"\" | \"trimSnaps\" | \"keepSnaps\">",
      "defaultValue": "''",
      "required": false,
      "description": "Clear leading and trailing empty space that causes excessive scrolling.\nUse trimSnaps to only use snap points that trigger scrolling or keepSnaps to keep them."
    },
    "controlNextProps": {
      "type": "IconButtonProps",
      "required": false,
      "description": "Props for next of the carousel control element."
    },
    "controlPrevProps": {
      "type": "IconButtonProps",
      "required": false,
      "description": "Props for previous of the carousel control element."
    },
    "controlProps": {
      "type": "IconButtonProps",
      "required": false,
      "description": "Props for carousel control element."
    },
    "defaultIndex": {
      "type": "number",
      "defaultValue": "0",
      "required": false,
      "description": "The initial index of the carousel slide."
    },
    "delay": {
      "type": "type ONLY_FOR_FORMAT =\n  | number\n  | ResponsiveObject<number>\n  | ColorModeArray<number>",
      "defaultValue": "4000",
      "required": false,
      "description": "The number for the autoplay interval of the carousel."
    },
    "dragFree": {
      "type": "type ONLY_FOR_FORMAT =\n  | boolean\n  | ResponsiveObject<boolean>\n  | ColorModeArray<boolean>",
      "defaultValue": false,
      "required": false,
      "description": "If `true`, momentum scrolling will be enabled."
    },
    "draggable": {
      "type": "type ONLY_FOR_FORMAT =\n  | boolean\n  | ResponsiveObject<boolean>\n  | ColorModeArray<boolean>",
      "defaultValue": true,
      "required": false,
      "description": "If `true`, carousel can be scrolled with mouse and touch interactions."
    },
    "includeGapInSize": {
      "type": "type ONLY_FOR_FORMAT =\n  | boolean\n  | ResponsiveObject<boolean>\n  | ColorModeArray<boolean>",
      "defaultValue": true,
      "required": false,
      "description": "If `true`, gap will be treated as part of the carousel slide size."
    },
    "index": {
      "type": "number",
      "required": false,
      "description": "The index of the carousel slide."
    },
    "indicatorsProps": {
      "type": "CarouselIndicatorsProps",
      "required": false,
      "description": "Props for carousel indicators element."
    },
    "innerProps": {
      "type": "HTMLUIProps<\"div\">",
      "required": false,
      "description": "Props for carousel inner element."
    },
    "inViewThreshold": {
      "type": "type ONLY_FOR_FORMAT =\n  | number\n  | ResponsiveObject<number>\n  | ColorModeArray<number>",
      "defaultValue": "0",
      "required": false,
      "description": "Choose a fraction representing the percentage portion of a slide that needs to be visible in order to be considered in view."
    },
    "loop": {
      "type": "type ONLY_FOR_FORMAT =\n  | boolean\n  | ResponsiveObject<boolean>\n  | ColorModeArray<boolean>",
      "defaultValue": true,
      "required": false,
      "description": "If `true`, infinite looping.\nAutomatically falls back to false if slide content isn't enough to loop."
    },
    "onChange": {
      "type": "(index: number) => void",
      "required": false,
      "description": "The callback invoked when carousel slide selected."
    },
    "onScrollProgress": {
      "type": "(progress: number) => void",
      "required": false,
      "description": "A callback that return the current scroll amount when the carousel is scrolled."
    },
    "orientation": {
      "type": "type ONLY_FOR_FORMAT =\n  | \"vertical\"\n  | \"horizontal\"\n  | ResponsiveObject<\"vertical\" | \"horizontal\">\n  | ColorModeArray<\"vertical\" | \"horizontal\">",
      "defaultValue": "'horizontal'",
      "required": false,
      "description": "The orientation of the carousel."
    },
    "size": {
      "defaultValue": "md",
      "type": "\"sm\" | \"md\" | \"lg\" | \"xl\"",
      "required": false,
      "description": "The size of the Carousel."
    },
    "skipSnaps": {
      "type": "type ONLY_FOR_FORMAT =\n  | boolean\n  | ResponsiveObject<boolean>\n  | ColorModeArray<boolean>",
      "defaultValue": false,
      "required": false,
      "description": "If `true`, allow the carousel to skip scroll snaps if it's dragged vigorously.\nNote that this option will be ignored if the dragFree option is set to true."
    },
    "slideSize": {
      "type": "number | \"px\" | (string & {}) | \"sm\" | \"md\" | \"lg\" | \"xl\" | \"2xl\" | \"-moz-initial\" | \"inherit\" | \"initial\" | \"revert\" | \"revert-layer\" | \"unset\" | \"-moz-fit-content\" | \"-moz-max-content\" | ... 64 more ... | ColorModeArray<...>",
      "required": false,
      "description": "The CSS `width` property."
    },
    "slidesToScroll": {
      "type": "type ONLY_FOR_FORMAT =\n  | number\n  | ResponsiveObject<number>\n  | ColorModeArray<number>",
      "defaultValue": "1",
      "required": false,
      "description": "The number of slides that should be scrolled with next or previous buttons."
    },
    "speed": {
      "type": "type ONLY_FOR_FORMAT =\n  | number\n  | ResponsiveObject<number>\n  | ColorModeArray<number>",
      "defaultValue": "10",
      "required": false,
      "description": "Adjusts scroll speed when triggered by any of the methods.\nHigher numbers enables faster scrolling."
    },
    "stopMouseEnterAutoplay": {
      "type": "type ONLY_FOR_FORMAT =\n  | boolean\n  | ResponsiveObject<boolean>\n  | ColorModeArray<boolean>",
      "defaultValue": true,
      "required": false,
      "description": "If `true`, autoplay will pause when the mouse entries over."
    },
    "variant": {
      "type": "string",
      "required": false,
      "description": "The variant of the Carousel."
    },
    "withControls": {
      "type": "type ONLY_FOR_FORMAT =\n  | boolean\n  | ResponsiveObject<boolean>\n  | ColorModeArray<boolean>",
      "defaultValue": true,
      "required": false,
      "description": "If `true`, display the carousel control buttons."
    },
    "withIndicators": {
      "type": "type ONLY_FOR_FORMAT =\n  | boolean\n  | ResponsiveObject<boolean>\n  | ColorModeArray<boolean>",
      "defaultValue": true,
      "required": false,
      "description": "If `true`, display the carousel indicator buttons."
    }
  },
  "CarouselSlide": {
    "index": { "type": "number", "required": false },
    "size": {
      "type": "number | \"px\" | (string & {}) | \"sm\" | \"md\" | \"lg\" | \"xl\" | \"2xl\" | \"-moz-initial\" | \"inherit\" | \"initial\" | \"revert\" | \"revert-layer\" | \"unset\" | \"-moz-fit-content\" | \"-moz-max-content\" | ... 64 more ... | ColorModeArray<...>",
      "required": false,
      "description": "The CSS `width` property."
    }
  },
  "CarouselControl": {
    "icon": {
      "type": "ReactElement<any, string | JSXElementConstructor<any>>",
      "required": false,
      "description": "The icon to be used in the button."
    },
    "isActive": {
      "type": "boolean",
      "defaultValue": false,
      "required": false,
      "description": "If `true`, the button is represented as active."
    },
    "isDisabled": {
      "type": "boolean",
      "defaultValue": false,
      "required": false,
      "description": "If `true`, the button is disabled."
    },
    "isLoading": {
      "type": "boolean",
      "defaultValue": false,
      "required": false,
      "description": "If `true`, the loading state of the button is represented."
    },
    "isRounded": {
      "type": "boolean",
      "defaultValue": false,
      "required": false,
      "description": "If `true`, the button will be perfectly round. Else, it'll be slightly round."
    }
  },
  "CarouselIndicators": {
    "component": {
      "type": "FC<{ index: number; isSelected: boolean }>",
      "required": false,
      "description": "The custom curousel indicator to use."
    }
  }
}
