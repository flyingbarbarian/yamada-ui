{
  "Button": {
    "colorScheme": {
      "defaultValue": "gray",
      "type": "\"primary\" | \"secondary\" | \"warning\" | \"danger\" | \"link\" | \"whiteAlpha\" | \"blackAlpha\" | \"gray\" | \"red\" | \"orange\" | \"yellow\" | \"green\" | \"teal\" | \"blue\" | \"cyan\" | \"purple\" | \"pink\"",
      "required": false,
      "description": "The visual color appearance of the component."
    },
    "isActive": { "type": "boolean", "required": false },
    "isDisabled": { "type": "boolean", "required": false },
    "isLoading": { "type": "boolean", "required": false },
    "leftIcon": {
      "type": "ReactElement<any, string | JSXElementConstructor<any>>",
      "required": false
    },
    "loadingIcon": {
      "type": "type ONLY_FOR_FORMAT =\n  | ReactElement<any, string | JSXElementConstructor<any>>\n  | LoadingVariant",
      "required": false
    },
    "loadingPlacement": { "type": "\"end\" | \"start\"", "required": false },
    "loadingText": { "type": "string", "required": false },
    "rightIcon": {
      "type": "ReactElement<any, string | JSXElementConstructor<any>>",
      "required": false
    },
    "size": {
      "defaultValue": "md",
      "type": "\"xs\" | \"sm\" | \"md\" | \"lg\"",
      "required": false,
      "description": "The size of the Button."
    },
    "variant": {
      "defaultValue": "solid",
      "type": "\"solid\" | \"outline\" | \"link\" | \"ghost\" | \"unstyled\"",
      "required": false,
      "description": "The variant of the Button."
    }
  },
  "ButtonGroup": {
    "direction": {
      "type": "type ONLY_FOR_FORMAT =\n  | FlexDirection\n  | ResponsiveObject<FlexDirection>\n  | ColorModeArray<FlexDirection>",
      "required": false
    },
    "isAttached": { "type": "boolean", "required": false },
    "isDisabled": { "type": "boolean", "required": false }
  },
  "IconButton": {
    "icon": { "type": "ReactElement<any, string | JSXElementConstructor<any>>", "required": false },
    "isActive": { "type": "boolean", "required": false },
    "isDisabled": { "type": "boolean", "required": false },
    "isLoading": { "type": "boolean", "required": false },
    "isRound": { "type": "boolean", "required": false }
  }
}
