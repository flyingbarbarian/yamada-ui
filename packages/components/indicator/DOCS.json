{
  "Indicator": {
    "colorScheme": {
      "defaultValue": "primary",
      "type": "\"primary\" | \"secondary\" | \"warning\" | \"danger\" | \"link\" | \"whiteAlpha\" | \"blackAlpha\" | \"gray\" | \"red\" | \"orange\" | \"yellow\" | \"green\" | \"teal\" | \"blue\" | \"cyan\" | \"purple\" | \"pink\"",
      "required": false,
      "description": "The visual color appearance of the component."
    },
    "inline": {
      "type": "type ONLY_FOR_FORMAT =\n  | boolean\n  | ResponsiveObject<boolean>\n  | ColorModeArray<boolean>",
      "defaultValue": false,
      "required": false,
      "description": "If `true`, set the indicator as an inline element."
    },
    "isDisabled": {
      "type": "boolean",
      "defaultValue": false,
      "required": false,
      "description": "If `true`, the indicator will be disabled."
    },
    "label": {
      "type": "type ONLY_FOR_FORMAT =\n  | string\n  | number\n  | boolean\n  | ReactElement<any, string | JSXElementConstructor<any>>\n  | Iterable<ReactNode>\n  | ReactPortal",
      "required": false,
      "description": "The indicator label to use."
    },
    "offset": {
      "type": "type ONLY_FOR_FORMAT =\n  | number\n  | ResponsiveObject<number>\n  | ColorModeArray<number>",
      "defaultValue": "0",
      "required": false,
      "description": "Changes position offset, usually used when element has border-radius."
    },
    "overflowCount": {
      "type": "number",
      "defaultValue": "99",
      "required": false,
      "description": "If `label` is of type number, the maximum number displayed."
    },
    "placement": {
      "type": "\"top\" | \"right\" | \"bottom\" | \"left\" | \"top-left\" | \"top-right\" | \"bottom-left\" | \"bottom-right\" | ResponsiveObject<\"top\" | \"right\" | \"bottom\" | \"left\" | \"top-left\" | \"top-right\" | \"bottom-left\" | \"bottom-right\"> | ColorModeArray<...>",
      "defaultValue": "'top-right'",
      "required": false,
      "description": "The placement of the indicator."
    },
    "showZero": {
      "type": "boolean",
      "defaultValue": true,
      "required": false,
      "description": "If `true`, display 0."
    },
    "size": {
      "defaultValue": "md",
      "type": "\"sm\" | \"md\" | \"lg\"",
      "required": false,
      "description": "The size of the Indicator."
    },
    "variant": {
      "defaultValue": "solid",
      "type": "\"solid\" | \"subtle\"",
      "required": false,
      "description": "The variant of the Indicator."
    },
    "withBorder": {
      "type": "boolean",
      "defaultValue": false,
      "required": false,
      "description": "If `true`, display the border of the indicator."
    }
  }
}
