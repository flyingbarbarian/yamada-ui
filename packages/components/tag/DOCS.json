{
  "Tag": {
    "colorScheme": {
      "defaultValue": "primary",
      "type": "\"primary\" | \"secondary\" | \"warning\" | \"danger\" | \"link\" | \"whiteAlpha\" | \"blackAlpha\" | \"gray\" | \"red\" | \"orange\" | \"yellow\" | \"green\" | \"teal\" | \"blue\" | \"cyan\" | \"purple\" | \"pink\"",
      "required": false,
      "description": "The visual color appearance of the component."
    },
    "isDisabled": {
      "type": "boolean",
      "defaultValue": false,
      "required": false,
      "description": "If `true`, the tag is disabled."
    },
    "leftIcon": {
      "type": "ReactElement<any, string | JSXElementConstructor<any>>",
      "required": false,
      "description": "Icon to be displayed to the left of the tag."
    },
    "onCloseClick": {
      "type": "MouseEventHandler<HTMLElement>",
      "required": false,
      "description": "Function to be executed when the close button is clicked."
    },
    "rightIcon": {
      "type": "ReactElement<any, string | JSXElementConstructor<any>>",
      "required": false,
      "description": "Icon to be displayed to the right of the tag."
    },
    "size": {
      "defaultValue": "md",
      "type": "\"sm\" | \"md\" | \"lg\"",
      "required": false,
      "description": "The size of the Tag."
    },
    "variant": {
      "defaultValue": "subtle",
      "type": "\"solid\" | \"subtle\" | \"outline\"",
      "required": false,
      "description": "The variant of the Tag."
    }
  }
}
